@using FitnessApp

<div class="top-row ps-3 navbar navbar-dark">
    <div class="container-fluid">
        <a class="navbar-brand" href="">FitnessApp</a>
    </div>
</div>

<input type="checkbox" title="Navigation menu" class="navbar-toggler" />

<div class="nav-scrollable" onclick="document.querySelector('.navbar-toggler').click()">
    <AuthorizeView>
        <NotAuthorized>
            <div class="nav-item px-3">
                <NavLink class="nav-link" href="" Match="NavLinkMatch.All">
                    <span class="oi oi-home" aria-hidden="true"></span> MyProfile
                </NavLink>
            </div>
        </NotAuthorized>
        <Authorized>
            <div class="nav-item px-3">
                <NavLink class="nav-link" href="" Match="NavLinkMatch.All">
                    <span class="oi oi-home" aria-hidden="true"></span> MyProfile
                </NavLink>
            </div>
            <div class="nav-item px-3">
                <NavLink class="nav-link" href="ticketTypes">
                    <span class="oi oi-plus" aria-hidden="true"></span> Memberships
                </NavLink>
            </div>
            <div class="nav-item px-3">
                <NavLink class="nav-link" href="clientTickets">
                    <span class="oi oi-list-rich" aria-hidden="true"></span> Client Memberships
                </NavLink>
            </div>
            <div class="nav-item px-3">
                <NavLink class="nav-link" href="clients">
                    <span class="oi oi-plus" aria-hidden="true"></span> Clients
                </NavLink>
            </div>

            <div class="nav-item px-3">
                <NavLink class="nav-link" href="entries">
                    <span class="oi oi-plus" aria-hidden="true"></span> Entries
                </NavLink>
            </div>

            <div class="nav-item px-3">
                <NavLink class="nav-link" href="entry">
                    <span class="oi oi-list-rich" aria-hidden="true"></span> Entry
                </NavLink>
            </div>
        </Authorized>
    </AuthorizeView>

</div>
